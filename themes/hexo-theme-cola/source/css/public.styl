li { list-style: none; }
a { text-decoration: none; color: #333; }
* { box-sizing: border-box; margin: 0; padding: 0; user-select: none; }
html {
  // filter: grayscale(1);
  font-family: "Noto Sans KR", "Apple SD Gothic Neo", AppleSDGothic, "Malgun Gothic", Gulim, Helvetica, Arial, sans-serif;
}
body {
  min-height: 100vh;
}
::-webkit-scrollbar {
  width: 0;
}

input {
  color: #666;
  padding: 7px 10px;
  outline: none;
  border: 1px solid #eee;
  font-size: 13px;
}



@import './mobile.styl';
